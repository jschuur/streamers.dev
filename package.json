{
  "name": "streamers.dev",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "postinstall": "prisma generate",
    "dev:update": "concurrently \"npm run dev\" \"npm run dev:update:users\"",
    "dev:update:users": "while true; do wget -q --spider 'http://localhost:3000/api/updateUsers/?force=1'; sleep 290; done",
    "update:users": "babel-node util/updateAllUsers.js",
    "update:users:full": "babel-node util/updateAllUsers.js --fullDetails",
    "update:users:prod": "dotenv -e .env.production babel-node util/updateAllUsers.js",
    "update:users:prod:full": "dotenv -e .env.production babel-node util/updateAllUsers.js -- --fullDetails",
    "studio": "prisma studio",
    "migrate:prod": "dotenv -e .env.production -- prisma migrate deploy",
    "db:dump": "pg_dump streamersdev > dbdump.sql"
  },
  "dependencies": {
    "@babel/core": "^7.14.3",
    "@fontsource/raleway": "^4.4.0",
    "@prisma/client": "^2.23.0",
    "babel-plugin-superjson-next": "^0.3.0",
    "console-stamp": "^3.0.2",
    "country-flag-icons": "^1.2.10",
    "date-fns": "^2.22.1",
    "dotenv": "^10.0.0",
    "iso-language-codes": "^1.0.8",
    "lodash": "^4.17.21",
    "minimist": "^1.2.5",
    "next": "^10.2.2",
    "pluralize": "^8.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "^4.2.0",
    "superjson": "^1.7.4",
    "twitch": "^4.5.5",
    "twitch-auth": "^4.5.5"
  },
  "devDependencies": {
    "@babel/node": "^7.14.2",
    "autoprefixer": "^10.2.5",
    "concurrently": "^6.1.0",
    "dotenv-cli": "^4.0.0",
    "postcss": "^8.3.0",
    "prisma": "^2.23.0",
    "tailwindcss": "^2.1.2"
  }
}
